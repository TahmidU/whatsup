{
  "arrowParents": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "indent": false,
  "operator-linebreak": false,
  "overrides": [
    {
      "files": ["resources/**/styles.tsx", "resources/**/styles.ts"],
      "options": {
        "indent": false,
        "operator-linebreak": false
      }
    }
  ]
}
